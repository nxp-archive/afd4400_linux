			jesdtx@0x80000{
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "fsl,d4400-jesd204-tx";
				ranges = <0x0 0x80000 0x4000>;
				reg = <0x00080000 0x4000>;

				complex-id = <0>;
				phy-gasket = <&phygasket1>;
				pll-id = <0>;
				first-serdes = <&serdes1 0>;

				transport0@0x0{
					reg = <0x0000000 0x7FC>;
					interrupts = <0 119 0x4>;
				};

				transport1@0x800{
